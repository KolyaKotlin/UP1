<Page x:Class="_122_Fedchenko.Pages.AdminPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:local="clr-namespace:_122_Fedchenko.Pages"
      mc:Ignorable="d"
      Title="AdminPage">

    <Grid>
        <Button x:Name="BtnTab1"
                Content="Таблица пользователей"
                HorizontalAlignment="Left"
                Margin="68,160,0,0"
                VerticalAlignment="Top"
                Height="41"
                Width="195"
                Click="BtnTab1_Click" />
        <Button x:Name="BtnTab2"
                Content="Таблица категорий"
                HorizontalAlignment="Center"
                Margin="0,160,0,0"
                VerticalAlignment="Top"
                Height="41"
                Width="173"
                Click="BtnTab2_Click" />
        <Button x:Name="BtnTab3"
                Content="Таблица платежей"
                HorizontalAlignment="Right"
                Margin="0,160,68,0"
                VerticalAlignment="Top"
                Height="41"
                Width="163"
                Click="BtnTab3_Click" />
        <Button x:Name="BtnTab4"
                Content="Диаграммы"
                HorizontalAlignment="Center"
                Margin="0,321,0,0"
                VerticalAlignment="Top"
                Height="41"
                Width="131"
                Click="BtnTab4_Click" />
    </Grid>
</Page>
